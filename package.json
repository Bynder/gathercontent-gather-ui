  "scripts": {
    "compile:css": "ENABLE_PURGE=true parcel build styles/prod.scss",
    "copy-tailwind": "tailwind build styles/prod-tailwind.css -o dist/styles/tailwind.css",
    "copy-styles": "echo Copying Sass settings... && npm run copy-tailwind && copyfiles styles/* dist/ && copyfiles styles/**/* dist/ && copyfiles lib/**/*.scss dist/ && copyfiles lib/**/**/*.scss dist/ && copyfiles lib/**/**/**/*.scss dist/ && copyfiles lib/**/**/**/**/*.scss dist/",
    "build:lib": "babel lib/ --out-dir dist/lib",
    "build:tailwind:config": "babel tailwind.config.js --out-dir dist",
    "build": "npm run build:lib && npm run build:tailwind:config",
    "build:with:styles": "npm run build && npm run copy-styles && npm run compile:css",
    "prepare": "npm run build:with:styles",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "build-storybook": "build-storybook",
    "test": "jest --notify || exit 0",
    "test:cc": "jest --clearCache",
    "test:watch": "jest --notify --watch",
    "test:ci": "jest --runInBand",
    "tdd": "npm run test:watch",
    "format:js": "prettier --write \"{lib,tests,stories/webapp}/**/*.js\"",
    "link:react": "cd ../app/webapp/node_modules/react && npm run link && cd ../../../../gather-ui && npm run link \"react\"",
    "unlink:react": "npm run unlink \"react\" && cd ../app/webapp/node_modules/react && npm run unlink && cd ../../../../gather-ui && npm run install --check-files",
    "lint": "eslint -c .eslintrc.json 'lib/**/*.js' 'tests/**/*.js'",
    "lint:fix": "eslint -c .eslintrc.json --fix 'lib/**/*.js' 'tests/**/*.js' 'stories/webapp/**/*.js'",
    "lint:check": "eslint --print-config . | eslint-config-prettier-check",
    "copy:styles:watch": "nodemon --exec \"npm run copy-styles\" --watch lib --watch styles -e scss --watch tailwind.config.js",
    "build:watch": "concurrently \"npm run copy:styles:watch\" \"npm run build:lib --watch --source-maps inline\" \"npm run build:tailwind:config --watch --source-maps inline\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{lib,tests,stories/webapp}/**/*.js": [
      "prettier --write",
      "eslint -c .eslintrc.json --fix",
      "git add"
    ]
  },
  "keywords": [
    "React",
    "UI Library"
  ],
  "license": "MIT",
  "files": [
    "dist"
  ],
