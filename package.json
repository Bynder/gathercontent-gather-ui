{
  "name": "gather-content-ui",
  "version": "0.2.0",
  "description": "GatherContent UI Library",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/gathercontent/gather-ui"
  },
  "bugs": {
    "url": "https://github.com/gathercontent/gather-ui/issues"
  },
  "author": "",
  "contributors": [{
    "name": "Kyle Harper",
    "email": "kyle@gathercontent.com"
  },{
    "name": "Ricardo MagalhÃ£es",
    "email": "ricardo@gathercontent.com"
  }],
  "scripts": {
    "autoprefixer": "node_modules/.bin/postcss --use autoprefixer dist/styles.css -o dist/styles.css",
    "pixrem": "node_modules/.bin/postcss --use pixrem dist/styles.css -o dist/styles.css",
    "copy-settings": "echo Copying Sass settings... && node_modules/.bin/copyfiles -f styles/settings/*.scss dist/settings",
    "styles": "echo Building Sass files... && node_modules/.bin/node-sass --output-style compressed styles/main.scss > dist/styles.css && npm run autoprefixer && npm run pixrem",
    "build": "node_modules/.bin/babel lib/ --out-dir dist/ && npm run styles",
    "prepublish": "npm run build",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "build-storybook": "build-storybook",
    "test": "mocha tests/** --compilers js:babel-register || exit 0",
    "test:watch": "mocha tests --watch --compilers js:babel-register",
    "tdd": "npm run test:watch",
    "lint": "./node_modules/.bin/eslint -c .eslintrc.json lib/* || exit 0"
  },
  "keywords": ["React", "UI Library"],
  "license": "MIT",
  "dependencies": {
    "bootstrap": "3.3.6",
    "classnames": "^2.2.5",
    "font-awesome": "git+ssh://git@github.com/FortAwesome/Font-Awesome.git",
    "normalize-scss": "^6.0.0",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2",
    "react-svg-inline": "^1.2.0",
    "svg-url-loader": "^1.1.1"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-inline-react-svg": "^0.2.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "file-loader": "^0.9.0",
    "jsdom": "^9.9.1",
    "jsdom-global": "^2.1.1",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "mocha": "^3.2.0",
    "node-sass": "^4.3.0",
    "pixrem": "^3.0.2",
    "postcss-cli-simple": "^1.0.3",
    "postcss-load-config": "^1.1.0",
    "postcss-loader": "^1.2.2",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.4.2",
    "sass-loader": "^4.1.1",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7"
  }
}
