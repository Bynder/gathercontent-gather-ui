  "scripts": {
    "copy-styles": "echo Copying Sass settings... && copyfiles styles/**/* dist/ && copyfiles styles/main.scss dist/ && copyfiles styles/bootstrap.scss dist/ && copyfiles lib/**/*.scss dist/ && copyfiles lib/**/**/*.scss dist/",
    "build": "babel lib/ --out-dir dist/lib",
    "build:with:styles": "yarn build && yarn run copy-styles",
    "prepublish": "yarn run build:with:styles",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "build-storybook": "build-storybook",
    "test": "jest --notify || exit 0",
    "test:cc": "jest --clearCache",
    "test:watch": "jest --notify --watch",
    "test:ci": "jest --runInBand",
    "tdd": "yarn run test:watch",
    "format:js": "prettier --write \"{lib,tests}/**/*.js\"",
    "lint": "eslint -c .eslintrc.json 'lib/**/*.js' 'tests/**/*.js'",
    "lint:fix": "eslint -c .eslintrc.json --fix 'lib/**/*.js' 'tests/**/*.js'",
    "lint:check": "eslint --print-config . | eslint-config-prettier-check",
    "copy:styles:watch": "nodemon --exec \"yarn copy-styles\" --watch lib --watch styles -e scss",
    "build:watch": "concurrently \"yarn copy:styles:watch\" \"yarn build --watch\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run test"
    }
  },
  "lint-staged": {
    "{lib,tests}/**/*.js": [
      "yarn run prettier --write",
      "eslint -c .eslintrc.json --fix",
      "git add"
    ]
  },
  "keywords": [
    "React",
    "UI Library"
  ],
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bootstrap": "3.4.1",
    "bootstrap-sass": "^3.3.7",
    "classnames": "^2.2.5",
    "debounce": "^1.1.0",
    "enzyme-adapter-react-16": "^1.2.0",
    "enzyme-adapter-react-16.1": "^1.0.0",
    "enzyme-adapter-react-16.2": "^1.0.0",
    "enzyme-adapter-react-16.3": "^1.0.0",
    "enzyme-adapter-utils": "^1.5.0",
    "font-awesome": "^4.7.0",
    "linkifyjs": "^2.1.6",
    "moment": "^2.21.0",
    "normalize-scss": "^6.0.0",
    "object-assign": "^4.1.1",
    "pluralize": "^5.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-bootstrap": "0.32.1",
    "react-day-picker": "^7.1.1",
    "react-dom": "^16.0.0",
    "react-mentions": "2.2.0",
    "react-svg-inline": "^1.2.0",
    "svg-url-loader": "^1.1.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.7",
    "@storybook/react": "3.0.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-inline-react-svg": "^0.2.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "concurrently": "^4.1.0",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.26.1",
    "enzyme": "^3.5.0",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.11.0",
    "file-loader": "^0.9.0",
    "husky": "^1.3.1",
    "jest": "^23.5.0",
    "less": "^2.7.2",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.3.0",
    "nodemon": "^1.18.11",
    "pixrem": "^3.0.2",
    "postcss-cli-simple": "^1.0.3",
    "postcss-load-config": "^1.1.0",
    "postcss-loader": "^1.2.2",
    "prettier": "1.16.4",
